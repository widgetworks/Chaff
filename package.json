{
  "name": "@wiwo/chaff",
  "version": "2.0.0",
  "description": "Easy object mocking for Typescript (and Javascript)",
  "main": "dist/Chaff.js",
  "typings": "dist/Chaff.d.ts",
  "scripts": {
    "build": "npm run build:lib",
    "build:lib": "tsc -p src/Chaff/tsconfig.json",
    "build:test": "tsc -p src/tests/tsconfig.json",
    "build:all": "npm run build && npm run build:test",
    "test": "mocha src/tests/ChaffTests.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/widgetworks/Chaff.git"
  },
  "keywords": [
    "Chaff",
    "Testing",
    "Mocking",
    "Mock",
    "Typescript"
  ],
  "author": "Adam Beck",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Gwash3189/Chaff/issues"
  },
  "homepage": "https://github.com/Gwash3189/Chaff",
  "devDependencies": {
    "expect": "^1.20.2",
    "mocha": "^5.2.0",
    "typescript": "^3.7.2"
  }
}
